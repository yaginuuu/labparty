<h1>Listing attends</h1>
<%= debug attend %>
<table>
  <thead>
    <tr>
      <th>Attend</th>
      <th>User</th>
      <th>Party</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @attends.each do |attend| %>
    <tr>
      <td><%= attend.attend %></td>
      <td><%= attend.user_id %></td>
      <td><%= attend.party_id %></td>
      <td><%= link_to 'Show', attend %></td>
      <td><%= link_to 'Edit', edit_attend_path(attend) %></td>
      <td><%= link_to 'Destroy', attend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Attend', new_attend_path %>
